// wood

while True:
	for i in range(get_world_size()):
		plant(Entities.Bush)
		move(North)
	for i in range(1):
		move(East)
	for i in range(get_world_size()):
		plant(Entities.Bush)
		move(North)
	for i in range(1):
		move(East)
	for i in range(get_world_size()):
		plant(Entities.Bush)
		move(North)
	for i in range(1):
		move(East)
		
	for i in range(get_world_size()):
		if can_harvest():
			harvest()
			move(North)
	for i in range(1):
		move(East)
	for i in range(get_world_size()):
		if can_harvest():
			harvest()
			move(North)
	for i in range(1):
		move(East)
	for i in range(get_world_size()):
		if can_harvest():
			harvest()
			move(North)
	for i in range(1):
		move(East)
	
	

// v1 Carrots 3x3

while True:
	for i in range(get_world_size()):
		till()
		plant(Entities.Carrot)
		move(North)
	for i in range(1):
		move(East)
	for i in range(get_world_size()):
		till()
		plant(Entities.Carrot)
		move(North)
	for i in range(1):
		move(East)
	for i in range(get_world_size()):
		till()
		plant(Entities.Carrot)
		move(North)
	for i in range(1):
		move(East)
		
	for i in range(get_world_size()):
			harvest()
			move(North)
	for i in range(1):
			move(East)
			do_a_flip()
	for i in range(get_world_size()):
			harvest()
			move(North)
	for i in range(1):
			move(East)
			do_a_flip()
	for i in range(get_world_size()):
			harvest()
			move(North)
	for i in range(1):
			move(East)
			do_a_flip()
	


